{
    "name": "magiccap",
    "version": "2.0.0b2",
    "description": "A cross platform image capturing suite.",
    "main": "./dist/initialiser.js",
    "author": "Jake Gealer & Rhys O'Kane",
    "license": "MPL-2.0",
    "repository": {
        "type": "git",
        "url": "git://github.com/JakeMakesStuff/MagicCap.git"
    },
    "dependencies": {
        "@icetee/ftp": "github:icetee/node-ftp#feature/rewrite",
        "@sentry/electron": "0.17.1",
        "aperture": "^5.2.0",
        "asciiart-logo": "0.2.6",
        "async-child-process": "1.1.1",
        "async-lock": "^1.2.0",
        "better-sqlite3": "5.4.0",
        "chainfetch": "1.2.2",
        "electron-progressbar": "github:whiteadi/electron-progressbar",
        "emojilib": "^2.4.0",
        "express": "^4.16.4",
        "fs-nextra": "0.3.5",
        "googleapis": "github:MagicCap/google-api-nodejs-client-magiccap",
        "jimp": "^0.6.4",
        "magicimports": "1.0.0",
        "markdown-it": "^8.4.2",
        "mime-types": "^2.1.24",
        "moment": "^2.22.2",
        "node-fetch": "2.6.0",
        "node-machine-id": "1.1.10",
        "pofile": "1.0.11",
        "promise-ftp": "^1.3.5",
        "request": "^2.88.0",
        "request-progress": "^3.0.0",
        "safe-eval": "0.4.1",
        "sharp": "0.22.1",
        "ssh2-sftp-client": "^2.5.0",
        "streamifier": "^0.1.1",
        "sudo-prompt": "^8.2.3",
        "temp-dir": "^2.0.0",
        "tiny-s3-uploader": "1.0.0",
        "uuid": "3.3.2",
        "vue": "2.5.21",
        "ws": "^7.0.0"
    },
    "devDependencies": {
        "@types/node": "12.0.4",
        "@typescript-eslint/eslint-plugin": "1.9.0",
        "@typescript-eslint/parser": "1.9.0",
        "color": "^3.1.2",
        "electron": "5.0.2",
        "electron-builder": "20.26.1",
        "electron-installer-dmg": "2.0.0",
        "electron-packager": "13.0.1",
        "eslint": "5.3.0",
        "lodash": ">=4.17.11",
        "mkdirp": "0.5.1",
        "typescript": "3.5.1",
        "walk": "2.3.14"
    },
    "scripts": {
        "copy": "node ./static_copy.js",
        "postinstall": "electron-builder install-app-deps --arch x64",
        "lint": "eslint \"./src/**/*.js\"",
        "lint-and-fix": "eslint \"./src/**/*.js\" --fix",
        "maccompile": "npm run build && electron-packager . MagicCap --ignore='(^\\/branding)|(^\\/src)|(\\/static_copy\\.js)' --no-deref-symlinks --platform darwin --arch x64 --out ./build --overwrite --icon ./dist/icons/app.icns && electron-installer-dmg ./build/MagicCap-darwin-x64/MagicCap.app MagicCap --out=./build --icon=./dist/icons/app.icns",
        "linuxcompile": "npm run build && electron-packager . MagicCap --ignore='(^\\/branding)|(^\\/src)|(\\/static_copy\\.js)' --platform linux --arch x64 --out ./build --overwrite --icon ./dist/icons/app.ico",
        "dev-update-electron": "sudo npm uninstall -g electron --unsafe-perm=true && sudo npm i -g electron --unsafe-perm=true",
        "dev-run": "npm run build && electron .",
        "build": "rm -rf dist/ && tsc && npm run copy",
        "dev-run-capturedev": "npm run build && electron . -captureDev",
        "dev-colors": "electron ./src/gui/css/colors.js -dark && electron ./src/gui/css/colors.js -light"
    }
}
